{
  "expo": {
    "name": "BinTracker",
    "slug": "BinTracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/lo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/lo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/lo.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/lo.pngg"
    },
    "plugins": [
      "expo-secure-store"
    ]
  }
}
